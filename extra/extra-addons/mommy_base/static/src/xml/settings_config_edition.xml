<?xml version="1.0" encoding="UTF-8"?>
<templates xml:space="preserve">
    <t t-inherit="res_config_edition" t-inherit-mode="extension">
        <xpath expr="//div[@class='tab-content']" position="inside">
            <div role="tabpanel" id="settings" class="tab-pane active text-muted o_web_settings_compact_subtitle">
                <small>Technical Support: <a target="_blank" href="https://odoohub.com.cn" style="text-decoration: underline;">青岛欧姆网络科技有限公司</a> </small>
            </div>
        </xpath>
    </t>

    <t t-inherit="web.MobileAppsFunnel" t-inherit-mode="extension">
        <xpath expr="//div[@class='d-flex']" position="inside">
            <div>
                <a class='d-block mx-auto' href="https://odoohub.com.cn" target="blank">
                    <img alt="Odoo Hub" class="d-block mx-auto img img-fluid" src="/mommy_base/static/description/public.svg"/>
                </a>
            </div>
        </xpath>
    </t>
</templates>