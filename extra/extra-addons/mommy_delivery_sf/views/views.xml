<odoo>
  <data>
    <record id="view_sf_route_detail" model="ir.ui.view">
      <field name="name">SF Routes Detail</field>
      <field name="model">stock.picking</field>
      <field name="inherit_id" ref="stock_delivery.view_picking_withcarrier_out_form"/>
      <field name="arch" type="xml">
        <xpath expr="//div[@name='tracking']" position="after">
          <field name="delivery_type" invisible="1"/>
          <field name="sf_express_type_select" invisible="1"/>
          <field name="sf_payment_select" invisible="1"/>
          <field name="sf_allow_manual" invisible="1"/>
          <field name="sf_cargo_detail" invisible="delivery_type != 'sf'"/>
          <field name="sf_express_type" readonly="state in ['done','cancel']" invisible="delivery_type != 'sf' or not sf_express_type_select"/>
          <field name="sf_payment_method" readonly="state in ['done','cancel']" invisible="delivery_type != 'sf' or not sf_payment_select"/>
          <field name="sf_declared_value" invisible="delivery_type != 'sf'"/>
          <field name="sf_declared_currency" invisible="delivery_type != 'sf'"/>
          <label for="carrier_tracking_detail" invisible="delivery_type != 'sf'"/>
          <div class="o_row">
            <field name="carrier_tracking_detail"/>
            <button name="update_tracking_detail" type="object" string="Update" class="fa fa-arrow-right oe_link" invisible="not carrier_tracking_detail or delivery_type != 'sf'"/>
          </div>
        </xpath>
        <button name="print_return_label" position="after">
          <button name="btn_sf" string="Order SF" type="object" class="oe_highlight" invisible="sf_allow_manual and not carrier_tracking_ref"/>
        </button>
        <button name="cancel_shipment" position="after">
          <button name="cancel_shipment" string="cancel" type="object" class="oe_link" invisible="not carrier_tracking_ref and (not sf_allow_manual or state in ['done','cancel'])"/>
        </button>
      </field>
    </record>
  </data>
</odoo>